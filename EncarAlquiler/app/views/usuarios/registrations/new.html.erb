<div class="container espacio">
  <div class="row">
    <div class="col-lg-8" class="img-responsive">
            <img src="../img/adm.jpg" style="padding-top: 100px;" width="500" height="400" >
    </div>
    <div class="col-lg-3">
      <div class="page-header">
        <h1 id="type">Registrarse</h1>
      </div>
      <%= bootstrap_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>
      <div class="field text-muted">
        <%= f.email_field :email, autofocus: true %>
      </div>
      <div class="field text-muted">
        <% if @minimum_password_length %>
        <em>(<%= @minimum_password_length %> caracteres minimos)</em>
        <% end %>
        <%= f.password_field :password, autocomplete: "off" %>
      </div>
      <div class="field text-muted">
        <%= f.password_field :password_confirmation, autocomplete: "off" %>
      </div>
      <div class="actions">
        <%= button_tag(type: 'submit', class: "btn btn-primary btn-block") do %>
          <span class="glyphicon glyphicon-user"></span> Registrarse
        <% end %>
      </br>
      </div>
      <% end %>
      <%= render "usuarios/shared/links" %>
    </div>
  </div>
</div>

